{
    "name": "ketcau/ketcau",
    "description": "e-frontier systems open platform.",
    "minimum-stability": "stable",
    "license": "GPL-2.0-only",
    "type": "project",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "ext-intl": "*",
        "composer/ca-bundle": "^1.3",
        "composer/composer": "^2.6",
        "doctrine/annotations": "^2.0",
        "doctrine/common": "^3.4",
        "doctrine/doctrine-bundle": "*",
        "doctrine/doctrine-fixtures-bundle": "*",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.16",
        "exercise/htmlpurifier-bundle": "*",
        "knplabs/knp-paginator-bundle": "*",
        "monolog/monolog": "^3.5",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.24",
        "psr/cache": "^3.0",
        "psr/container": "*",
        "psr/log": "^3.0",
        "psr/simple-cache": "^3.0",
        "setasign/fpdi": "^2.5",
        "skorp/detect-incompatible-samesite-useragents": "^1.0",
        "symfony/asset": "6.3.*",
        "symfony/cache": "6.3.*",
        "symfony/config": "6.3.*",
        "symfony/console": "6.3.*",
        "symfony/css-selector": "6.3.*",
        "symfony/debug-bundle": "6.3.*",
        "symfony/dependency-injection": "6.3.*",
        "symfony/doctrine-messenger": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/expression-language": "6.3.*",
        "symfony/flex": "^2",
        "symfony/form": "6.3.*",
        "symfony/framework-bundle": "6.3.*",
        "symfony/http-client": "6.3.*",
        "symfony/http-foundation": "6.3.*",
        "symfony/http-kernel": "6.3.*",
        "symfony/intl": "6.3.*",
        "symfony/mailer": "6.3.*",
        "symfony/maker-bundle": "^1.52",
        "symfony/mime": "6.3.*",
        "symfony/monolog-bridge": "6.3.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "6.3.*",
        "symfony/password-hasher": "6.3.*",
        "symfony/process": "6.3.*",
        "symfony/property-access": "6.3.*",
        "symfony/property-info": "6.3.*",
        "symfony/routing": "6.3.*",
        "symfony/runtime": "6.3.*",
        "symfony/security-bundle": "6.3.*",
        "symfony/serializer": "6.3.*",
        "symfony/string": "6.3.*",
        "symfony/templating": "6.3.*",
        "symfony/translation": "6.3.*",
        "symfony/twig-bundle": "6.3.*",
        "symfony/validator": "6.3.*",
        "symfony/web-link": "6.3.*",
        "symfony/webapp-meta": "^1.0",
        "symfony/webpack-encore-bundle": "^1.12",
        "symfony/yaml": "6.3.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.8",
        "twig/twig": "^2.12|^3.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "files": [
            "src/Ketcau/Resource/functions/log.php",
            "src/Ketcau/Resource/functions/env.php",
            "src/Ketcau/Resource/functions/trans.php"
        ],
        "psr-4": {
            "Ketcau\\Entity\\": "app/proxy/entity",
            "Customize\\": "app/Customize",
            "Ketcau\\": "src/Ketcau"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ketcau\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.3.*"
        },
        "bin-dir": "bin",
        "src-dir": "src/Ketcau",
        "config-dir": "config",
        "public-dir": "html",
        "symfony-web-dir": "html",
        "entity-dir": "src/Ketcau/Entity"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "*",
        "doctrine/data-fixtures": "^1.7",
        "symfony/stopwatch": "6.3.*",
        "symfony/web-profiler-bundle": "6.3.*"
    }
}
